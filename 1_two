class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:
        
        my_hash = {}
        
        for index, number in enumerate(nums):
            diff = target - number
            if diff in my_hash:
                return [my_hash[diff], index]
            my_hash[number] = index
        
        return
        

                
      
                
            

        
        